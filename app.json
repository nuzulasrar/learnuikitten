{
  "expo": {
    "name": "Talent Book",
    "slug": "talentbook",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/images/appicon/icon.png",
    "splash": {
      "image": "./assets/images/appicon/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#64a5c2"
    },
    "android": {
      "package": "com.rubysb.talentbook"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with job agencies to hire you."
        }
      ]
    ]
  }
}